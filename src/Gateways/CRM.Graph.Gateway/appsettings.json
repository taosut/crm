{
  "App": {
    "Name": "GraphQL-Gateway"
  },
  "Cors": {
    "Origins": "http://localhost:5102"
  },
  "Jaeger": {
    "Enabled": false,
    "ServiceName": "GraphQL-Gateway",
    "UdpHost": "localhost",
    "UdpPort": 6831,
    "MaxPacketSize": 0,
    "Sampler": "const"
  },
  "Services": {
    "ContactService": {
      "ServiceName": "Contact Service",
      "Url": "http://localhost:5001"
    }
  },
  "Logging": {
    "consoleEnabled": true,
    "Seq": {
      "Enabled": false,
      "Url": "http://localhost:5340/",
      "ApiKey": "wcJNfB6LIwjN29sSwPlG"
    },
    "Loki": {
      "Enabled": false,
      "Url": "http://localhost:3100/"
    },
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning",
        "Microsoft.Hosting.Lifetime": "Information",
        "Microsoft.EntityFrameworkCore": "Debug",
        "HotChocolate": "Error"
      }
    }
  },
  "metrics": {
    "enabled": false,
    "prometheusEnabled": false,
    "influxEnabled": false,
    "influxUrl": "http://localhost:8086",
    "database": "crm-metrics",
    "env": "development",
    "interval": 5,
    "tags": {
      "app": "graphQL-gateway",
      "env": "local"
    }
  },
  "PathBase": "/gateway",
  "AllowedHosts": "*"
}
